{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["plugin:react/recommended", "standard", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "prettier"],
  "rules": {
    "no-use-before-define": "off",
    "react/react-in-jsx-scope": "off",
    "space-before-function-paren": "off",
    "prettier/prettier": "error",
    "no-undef": "error",
    "react/prop-types": "off",
    "no-console": "error"
  }
}
